<view><view class="cu-bar search bg-white"><view class="search-form round"><text class="cuIcon-search"></text><input data-name="searchValue" name="searchValue" adjust-position="{{false}}" placeholder="请输入单元编号" confirm-type="search" data-event-opts="{{[['focus',[['onChange',['$event']]]],['blur',[['onSearch',['$event']]]],['input',[['__set_model',['','searchValue','$event',[]]]]]]}}" value="{{searchValue}}" bindfocus="__e" bindblur="__e" bindinput="__e"/></view><view class="action"><button data-event-opts="{{[['tap',[['onSearch',['$event']]]]]}}" class="cu-btn bg-green shadow-blur round" bindtap="__e">搜索</button></view></view><view class="cu-list menu"><block wx:for="{{units}}" wx:for-item="unit" wx:for-index="idx" wx:key="idx"><view class="cu-item" label="{{unit.unitId}}"><view class="content padding-tb-sm" is-link="{{true}}" data-event-opts="{{[['tap',[['chooseUnit',['$0'],[[['units','',idx]]]]]]]}}" bindtap="__e"><view><text class="cuIcon-clothesfill text-blue margin-right-xs"></text>{{''+unit.unitNum+"单元"}}</view><view class="text-gray text-sm"><text class="cuIcon-infofill margin-right-xs"></text>{{''+unit.unitId}}</view></view></view></block></view></view>