<view><view class="myfixed"><view class="cu-bar search bg-white"><view class="search-form round"><text class="cuIcon-search"></text><input adjust-position="{{false}}" type="text" placeholder="搜索房屋" confirm-type="search" data-event-opts="{{[['input',[['__set_model',['','keyWord','$event',[]]]]]]}}" value="{{keyWord}}" bindinput="__e"/></view><view class="action"><button data-event-opts="{{[['tap',[['searchRentRoom',['$event']]]]]}}" class="cu-btn bg-green shadow-blur round" bindtap="__e">搜索</button></view></view><scroll-view class="bg-white nav" scroll-x="{{true}}"><view class="flex text-center"><block wx:for="{{title}}" wx:for-item="item" wx:for-index="index" wx:key="status"><view class="{{['cu-item flex-sub',index==TabCur?'text-orange cur':'']}}" data-id="{{item.status}}" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" bindtap="__e">{{''+item.name+''}}</view></block></view></scroll-view></view><view class="cu-card article no-card"><view class="title-class" style="margin-top:82px;"></view><block wx:for="{{rents}}" wx:for-item="item" wx:for-index="index"><view class="cu-item shadow border-bottom"><view class="content"><image src="../../static/images/rentimage.jpg" mode="aspectFill"></image><view class="desc"><view class="title-class" style="margin-left:5px;">{{item.rentingTitle}}</view><view class="text-content"><view class="padding-sm flex flex-wrap"><view class="cu-tag line-green sm">{{item.rentingType==3344?'整租':'合租'}}</view><view class="cu-tag line-orange sm">{{item.paymentTypeName}}</view><view class="cu-tag line-olive sm">{{item.rentingDesc}}</view></view><view style="margin-left:5px;"><text class="text-red">{{item.price}}</text></view></view></view></view></view></block></view></view>