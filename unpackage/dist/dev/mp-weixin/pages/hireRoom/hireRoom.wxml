<view><view class="block__title">房屋信息</view><view class="cu-form-group"><view class="title">出租标题</view><input class="text-right" placeholder="如香格里拉豪华大单间" data-event-opts="{{[['input',[['__set_model',['','rentingTitle','$event',[]]]]]]}}" value="{{rentingTitle}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">房屋信息</view><picker bindchange="__e" value="{{index}}" range="{{roomCloums}}" data-event-opts="{{[['change',[['roomChange',['$event']]]]]}}"><view class="picker">{{''+(roomName?roomName:'请选择')+''}}</view></picker></view><block wx:if="{{builtUpArea!=''}}"><view class="cu-form-group"><view class="title">房屋面积</view><input style="text-align:right;" disabled="disabled" data-event-opts="{{[['input',[['__set_model',['','builtUpArea','$event',[]]]]]]}}" value="{{builtUpArea}}" bindinput="__e"/></view></block><block wx:if="{{apartment!=''}}"><view class="cu-form-group"><view class="title">户型</view><input style="text-align:right;" disabled="disabled" data-event-opts="{{[['input',[['__set_model',['','apartment','$event',[]]]]]]}}" value="{{apartment}}" bindinput="__e"/></view></block><block wx:if="{{layer!=''}}"><view class="cu-form-group"><view class="title">楼层</view><input style="text-align:right;" disabled="disabled" data-event-opts="{{[['input',[['__set_model',['','layer','$event',[]]]]]]}}" value="{{layer}}" bindinput="__e"/></view></block><view class="block__title">出租信息</view><view class="cu-form-group"><view class="title">付费类型</view><picker id="paymentType" bindchange="__e" value="{{paymentTypeIndex}}" range-key="paymentTypeName" range="{{paymentTypes}}" data-event-opts="{{[['change',[['paymentTypeChange',['$event']]]]]}}"><view class="picker">{{''+paymentTypes[paymentTypeIndex].paymentTypeName+''}}</view></picker></view><view class="cu-form-group"><view class="title">租金</view><input class="text-right" placeholder="请输入每月租金" data-event-opts="{{[['input',[['__set_model',['','price','$event',[]]]]]]}}" value="{{price}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">出租方式</view><picker id="rentingType" bindchange="__e" value="{{rentingTypeIndex}}" range-key="rentingTypeName" range="{{rentingTypes}}" data-event-opts="{{[['change',[['rentingTypeChange',['$event']]]]]}}"><view class="picker">{{''+rentingTypes[rentingTypeIndex].rentingTypeName+''}}</view></picker></view><view class="cu-form-group"><view class="title">服务费</view><input class="text-right" disabled="disabled" data-event-opts="{{[['input',[['__set_model',['','servicePrice','$event',[]]]]]]}}" value="{{servicePrice}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">入住日期</view><picker id="checkIn" bindchange="__e" value="{{checkInIndex}}" range-key="checkInName" range="{{checkIns}}" data-event-opts="{{[['change',[['checkInChange',['$event']]]]]}}"><view class="picker">{{''+checkIns[checkInIndex].checkInName+''}}</view></picker></view><view class="cu-form-group margin-top"><textarea placeholder="请输入描述信息" data-event-opts="{{[['input',[['__set_model',['','rentingDesc','$event',[]]]]]]}}" value="{{rentingDesc}}" bindinput="__e"></textarea></view><view class="block__title">相关图片</view><view class="cu-bar bg-white "><view class="action">图片上传</view><view class="action">{{''+imgList.length+'/4'}}</view></view><view class="cu-form-group"><view class="grid col-4 grid-square flex-sub"><block wx:for="{{imgList}}" wx:for-item="img" wx:for-index="index" wx:key="index"><view class="bg-img" bindtap="ViewImage" data-url="{{imgList[index]}}"><image src="{{imgList[index]}}" mode="aspectFill"></image><view class="cu-tag bg-red" data-index="{{index}}" data-event-opts="{{[['tap',[['deleteImage',[index]]]]]}}" bindtap="__e"><text class="cuIcon-close"></text></view></view></block><block wx:if="{{imgList.length<4}}"><view data-event-opts="{{[['tap',[['ChooseImage',['$event']]]]]}}" class="solids" bindtap="__e"><text class="cuIcon-cameraadd"></text></view></block></view></view><view class="button_up_blank"></view><view class="flex flex-direction"><button data-event-opts="{{[['tap',[['submitHireRoom']]]]}}" class="cu-btn bg-green margin-tb-sm lg" bindtap="__e">提交</button></view></view>